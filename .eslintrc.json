{
    "extends": "airbnb",
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "prefer-arrow-callback": "off",
        "func-names": "off"
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    }
}
